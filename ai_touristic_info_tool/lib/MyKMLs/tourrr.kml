<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2" xmlns:gx="http://www.google.com/kml/ext/2.2">
  <Document>
    <name>ScreenOverlay Tour</name>
    
    <!-- ScreenOverlay 1 -->
    <ScreenOverlay id="overlay1">
      <name>Overlay 1</name>
      <overlayXY x="0" y="1" xunits="fraction" yunits="fraction"/>
      <screenXY x="0" y="1" xunits="fraction" yunits="fraction"/>
      <rotationXY x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
      <size x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
      <description>
        <![CDATA[
          <html>
            <head>
              <style>
                .overlay-content {
                  text-align: center;
                  color: white;
                  font-family: Arial, sans-serif;
                }
              </style>
            </head>
            <body>
              <div class="overlay-content">
                <h1>Overlay Title</h1>
                <p>This is a sample description for the screen overlay one.</p>
              </div>
            </body>
          </html>
        ]]>
      </description>
    </ScreenOverlay>
    
    <!-- ScreenOverlay 2 -->
    <ScreenOverlay id="overlay2">
      <name>Overlay 2</name>
      <overlayXY x="1" y="1" xunits="fraction" yunits="fraction"/>
      <screenXY x="1" y="1" xunits="fraction" yunits="fraction"/>
      <rotationXY x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
      <size x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
      <description>
        <![CDATA[
          <html>
            <head>
              <style>
                .overlay-content {
                  text-align: center;
                  color: white;
                  font-family: Arial, sans-serif;
                }
              </style>
            </head>
            <body>
              <div class="overlay-content">
                <h1>Overlay Title</h1>
                <p>This is a sample description for the screen overlay two.</p>
              </div>
            </body>
          </html>
        ]]>
      </description>
    </ScreenOverlay>

    <!-- ScreenOverlay 3 -->
    <ScreenOverlay id="overlay3">
      <name>Overlay 3</name>
      <overlayXY x="0" y="0" xunits="fraction" yunits="fraction"/>
      <screenXY x="0" y="0" xunits="fraction" yunits="fraction"/>
      <rotationXY x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
      <size x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
      <description>
        <![CDATA[
          <html>
            <head>
              <style>
                .overlay-content {
                  text-align: center;
                  color: white;
                  font-family: Arial, sans-serif;
                }
              </style>
            </head>
            <body>
              <div class="overlay-content">
                <h1>Overlay Title</h1>
                <p>This is a sample description for the screen overlay three.</p>
              </div>
            </body>
          </html>
        ]]>
      </description>
    </ScreenOverlay>

    <!-- Tour -->
    <gx:Tour>
      <name>ScreenOverlay Tour</name>
      <gx:Playlist>
        <!-- Fly to Location 1 -->
        <gx:FlyTo>
          <gx:duration>3.0</gx:duration>
          <gx:flyToMode>smooth</gx:flyToMode>
          <LookAt>
            <longitude>-122.0839597145766</longitude>
            <latitude>37.42222904525232</latitude>
            <altitude>0</altitude>
            <heading>0</heading>
            <tilt>0</tilt>
            <range>1000</range>
            <gx:altitudeMode>relativeToGround</gx:altitudeMode>
          </LookAt>
        </gx:FlyTo>
        <!-- Show Overlay 1 -->
        <gx:AnimatedUpdate>
          <gx:duration>0.5</gx:duration>
          <Update>
            <targetHref/>
            <Change>
              <ScreenOverlay targetId="overlay1">
                <visibility>1</visibility>
              </ScreenOverlay>
            </Change>
          </Update>
        </gx:AnimatedUpdate>
        <!-- Wait 5 seconds -->
        <gx:Wait>
          <gx:duration>5.0</gx:duration>
        </gx:Wait>
        <!-- Hide Overlay 1 -->
        <gx:AnimatedUpdate>
          <gx:duration>0.5</gx:duration>
          <Update>
            <targetHref/>
            <Change>
              <ScreenOverlay targetId="overlay1">
                <visibility>0</visibility>
              </ScreenOverlay>
            </Change>
          </Update>
        </gx:AnimatedUpdate>
        <!-- Fly to Location 2 -->
        <gx:FlyTo>
          <gx:duration>3.0</gx:duration>
          <gx:flyToMode>smooth</gx:flyToMode>
          <LookAt>
            <longitude>-122.0856545755255</longitude>
            <latitude>37.42199951478225</latitude>
            <altitude>0</altitude>
            <heading>0</heading>
            <tilt>0</tilt>
            <range>1000</range>
            <gx:altitudeMode>relativeToGround</gx:altitudeMode>
          </LookAt>
        </gx:FlyTo>
        <!-- Show Overlay 2 -->
        <gx:AnimatedUpdate>
          <gx:duration>0.5</gx:duration>
          <Update>
            <targetHref/>
            <Change>
              <ScreenOverlay targetId="overlay2">
                <visibility>1</visibility>
              </ScreenOverlay>
            </Change>
          </Update>
        </gx:AnimatedUpdate>
        <!-- Wait 5 seconds -->
        <gx:Wait>
          <gx:duration>5.0</gx:duration>
        </gx:Wait>
        <!-- Hide Overlay 2 -->
        <gx:AnimatedUpdate>
          <gx:duration>0.5</gx:duration>
          <Update>
            <targetHref/>
            <Change>
              <ScreenOverlay targetId="overlay2">
                <visibility>0</visibility>
              </ScreenOverlay>
            </Change>
          </Update>
        </gx:AnimatedUpdate>
        <!-- Fly to Location 3 -->
        <gx:FlyTo>
          <gx:duration>3.0</gx:duration>
          <gx:flyToMode>smooth</gx:flyToMode>
          <LookAt>
            <longitude>-122.0822035425683</longitude>
            <latitude>37.42223010000001</latitude>
            <altitude>0</altitude>
            <heading>0</heading>
            <tilt>0</tilt>
            <range>1000</range>
            <gx:altitudeMode>relativeToGround</gx:altitudeMode>
          </LookAt>
        </gx:FlyTo>
        <!-- Show Overlay 3 -->
        <gx:AnimatedUpdate>
          <gx:duration>0.5</gx:duration>
          <Update>
            <targetHref/>
            <Change>
              <ScreenOverlay targetId="overlay3">
                <visibility>1</visibility>
              </ScreenOverlay>
            </Change>
          </Update>
        </gx:AnimatedUpdate>
        <!-- Wait 5 seconds -->
        <gx:Wait>
          <gx:duration>5.0</gx:duration>
        </gx:Wait>
        <!-- Hide Overlay 3 -->
        <gx:AnimatedUpdate>
          <gx:duration>0.5</gx:duration>
          <Update>
            <targetHref/>
            <Change>
              <ScreenOverlay targetId="overlay3">
                <visibility>0</visibility>
              </ScreenOverlay>
            </Change>
          </Update>
        </gx:AnimatedUpdate>
      </gx:Playlist>
    </gx:Tour>
  </Document>
</kml>
